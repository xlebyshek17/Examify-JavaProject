<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examify.ui.controllers.AdminUsersController" stylesheets="@../styles/styles.css">
   <children>
      <TableView fx:id="userTable" AnchorPane.topAnchor="0"
                                    AnchorPane.bottomAnchor="0"
                                    AnchorPane.leftAnchor="0"
                                    AnchorPane.rightAnchor="0" >
        <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="150.0" text="Username" />
          <TableColumn fx:id="emailColumn" prefWidth="200.0" text="Email" />
            <TableColumn fx:id="isAdminColumn" prefWidth="100.0" text="IsAdmin" />
            <TableColumn fx:id="actionColumn" prefWidth="150.0" text="Action" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
